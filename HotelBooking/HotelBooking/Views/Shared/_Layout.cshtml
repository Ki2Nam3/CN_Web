    <link href="/Content/User/Plugins/Menu/styles-menu.css" rel="stylesheet" />
    <!-- comment css-->
    <link href="/Content/User/Content/css/comment.css" rel="stylesheet" />
    <!-- JS -->
    <script src="/Content/User/Plugins/jquery-ui/js/jquery-1.9.1.js"></script>
    <script src="/Content/User/Plugins/jquery-ui/js/jquery-ui-1.10.0.min.js"></script>
    <!-- script menu-->
    <script src="/Content/User/Plugins/Menu/script-menu.js"></script>
</head>

<body>
    <div id="bodychild">
        <div id="outercontainer">

            <!-- HEADER -->
            <div id="outerheader">
                <div id="headercontainer">
                    <div class="container" style="position: relative">
                        <div id="logo">
                            <a href="index.html"><img src="/Content/User/files/images/comment/logo.png" alt="GRAND HOTEL"></a>
                        </div>
                        <div id="navigation">

                            <div id='cssmenu'>
                                <ul>
                                    <li><a href="Index">Trang Chủ</a></li>
