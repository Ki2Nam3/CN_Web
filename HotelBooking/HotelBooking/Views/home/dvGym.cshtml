@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


            
            <div id="outerafterheader">
                <div class="container">
                    <div class="row">
                        <div class="twelve columns" id="afterheader">
                            <h1 class="pagetitle nodesc">Gym</h1>
                        </div>
                    </div>
                </div>
            </div>

            <div id="outermain">
                <div id="maincontainer">
                    <div class="container">
                        <div class="row">
                            <section id="maincontent">
                                <section id="content" class="twelve columns">
                                    <div id="ts-portfolio">
                                        <div class="row ">
                                            <section class="single content-detail">
                                                <section class="eight columns positionleft">
                                                    <article class="post">
                                                        <div class="articlecontainer">
                                                            <div id="tabbed-nav">
                                                                <!-- Tab Navigation Menu, to have seo friendly hashtags, add data-link attribute to each li tag and give it a unique id to apear in the url-->
                                                                <ul>
                                                                    <li data-link="Chi tiet"><a>Chi tiết</a></li>
                                                                </ul>
                                                                <!-- Content container -->
                                                                <div>
                                                                    <div>
                                                                        <div id="ts-portfolio"
                                                                            style="margin-bottom: 20px;">
                                                                            <div class="row ">
                                                                                <div class="columns one_half">
                                                                                    <div class="columns one_half"
                                                                                        style="padding: 0px 9px; margin-bottom: 22px;">
                                                                                        <div class="ts-pf-img">
                                                                                            <a class="pfzoom"
                                                                                                href="/Content/User/files/images/DichVu/gym/gym.jpg"
                                                                                                data-rel="prettyPhoto[mixed]"
                                                                                                rel="prettyPhoto[mixed]">
                                                                                                <img
                                                                                                    src="/Content/User/files/images/DichVu/gym/gym.jpg">
                                                                                            </a>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                                <div style="margin-top: 9px;"
                                                                                    class="columns one_half">
                                                                                    <p>Phòng tập Gym của Hội An Hotel &
                                                                                        Spa sở hữu một không gian tập
                                                                                        rộng rãi với đầy đủ trang thiết
                                                                                        bị hiện đại.
                                                                                        Tất cả những dụng cụ tập đều
                                                                                        được đầu tư hiện đại để đảm bảo
                                                                                        việc tập luyện của Quý khách
                                                                                        thuận tiện và hiệu
                                                                                        quả. Đội ngũ huấn luyện viên cá
                                                                                        nhân có hiểu biết chuyên sâu về
                                                                                        dinh dưỡng cũng như luyện tập
                                                                                        luôn hướng dẫn
                                                                                        bạn từ những động tác nhỏ nhất,
                                                                                        đến lộ trình dinh dưỡng và luyện
                                                                                        tập trong suốt quá trình bạn
                                                                                        sinh hoạt tại đây.
                                                                                    </p>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <script>
                                                                jQuery(document).ready(function ($) {
                                                                    /* jQuery activation and setting options for the tabs, also setting the the following:
                                                                - deeplinking: for deep-linking
                                                                - defaultTab: change it to one the new tab id*/
                                                                    $("#tabbed-nav").zozoTabs({
                                                                        theme: "silver",
                                                                        position: "top-left",
                                                                        orientation: "horizontal",
                                                                        deeplinking: true,
                                                                        defaultTab: 'Chi tiet'
                                                                    });

                                                                    jQuery("a[rel^='prettyPhoto']").prettyPhoto({ animationSpeed: 'slow', theme: 'pp_default', gallery_markup: '', slideshow: 2000, social_tools: '' });
                                                                });
                                                            </script>
                                                            <div class="clear"></div>
                                                        </div>
                                                    </article>
                                                    <div class="clear"></div>
                                                    <section class="other-content">
                                                        <div id="ts-portfolio">
                                                            <div class="row">
                                                                <h2 class="h2-other">dịch vụ liên quan</h2>
                                                                <div class="item one_third columns item-other">
                                                                    <div class="ts-pf-img">
                                                                        <a href="/Home/dvHoBoi">
                                                                            <img src="/Content/User/files/images/DichVu/hoboi/hoboi.jpg"
                                                                                alt="Hồ bơi">
                                                                        </a>
                                                                    </div>
                                                                    <div class="ts-pf-text">
                                                                        <h3 class="pftitle"><a
                                                                                href="/Home/dvHoBoi"
                                                                                title="Hồ bơi">Hồ bơi</a></h3>
                                                                    </div>
                                                                </div>
                                                                <div class="item one_third columns item-other">
                                                                    <div class="ts-pf-img">
                                                                        <a href="/Home/dvSpa">
                                                                            <img src="/Content/User/files/images/DichVu/spa/spa.jpg"
                                                                                alt="Spa">
                                                                        </a>
                                                                    </div>
                                                                    <div class="ts-pf-text">
                                                                        <h3 class="pftitle"><a href="/Home/dvSpa"
                                                                                title="Spa">Spa</a></h3>
                                                                    </div>
                                                                </div>
                                                                <div class="item one_third columns item-other">
                                                                    <div class="ts-pf-img">
                                                                        <a href="/Home/dvNhaHang">
                                                                            <img src="/Content/User/files/images/DichVu/nhahang/nhahang.jpg"
                                                                                alt="Nhà hàng">
                                                                        </a>
                                                                    </div>
                                                                    <div class="ts-pf-text">
                                                                        <h3 class="pftitle"><a
                                                                                href="/Home/dvNhaHang"
                                                                                title="Nhà hàng">Nhà hàng</a></h3>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="clear"></div>
                                                    </section>
                                                    <!-- end post -->
                                                </section>
                                                <section>
                                                    <aside id="sidebar" class="four columns positionright">
                                                        <div class="widget-area">
                                                            <ul>
                                                                <li id="text-3" class="widget-container widget_text">
                                                                    <div class="wrap-mode-check">
                                                                        <h3 class="widget-title">Đặt phòng khách sạn
                                                                        </h3>
                                                                        <div id="wrap-check-one">
                                                                            <form id="check-room">
                                                                                <div class="av-check-time">
                                                                                    <span>Ngày đến:</span> <input
                                                                                        class="checkdate" id="Arrive"
                                                                                        type="text">
                                                                                </div>
                                                                                <div class="av-check-time">
                                                                                    <span>Ngày đi: </span> <input
                                                                                        class="checkdate" id="Depart"
                                                                                        type="text">
                                                                                </div>
                                                                                <div class="av-persion">
                                                                                    <div class="av-item-pertion">
                                                                                        <span>Người lớn:</span>
                                                                                        <select id="av-adult"
                                                                                            name="Adult">
                                                                                            <option>1</option>
                                                                                            <option>2</option>
                                                                                            <option>3</option>
                                                                                            <option>4</option>
                                                                                            <option>5</option>
                                                                                        </select>

                                                                                    </div>
                                                                                    <div class="av-item-pertion">
                                                                                        <span>Trẻ em:</span>
                                                                                        <select id="av-child"
                                                                                            name="Child">
                                                                                            <option>0</option>
                                                                                            <option>1</option>
                                                                                            <option>2</option>
                                                                                            <option>3</option>
                                                                                            <option>4</option>
                                                                                            <option>5</option>
                                                                                        </select>
                                                                                    </div>
                                                                                </div>
                                                                                <input type="submit" id="av-buton"
                                                                                    value="Tìm phòng trống"
                                                                                    class="btn btn-primary"
                                                                                    style=" padding: 9px 20px; margin-top: 10px; width: 100%" />
                                                                            </form>
                                                                        </div>
                                                                    </div>
                                                                    <style>
                                                                        .av-check-time,
                                                                        .av-persion,
                                                                        .av-item-pertion {
                                                                            margin-bottom: 8px;
                                                                        }

                                                                        #check-room span {
                                                                            display: block;
                                                                            width: 86px;
                                                                            float: left;
                                                                        }

                                                                        #check-room input.checkdate,
                                                                        #check-room select {
                                                                            padding: 5px 10px;
                                                                            padding: 5px 10px;
                                                                            background: #ffffff;
                                                                            color: #121212;
                                                                            border: 1px solid #cacaca;
                                                                        }
                                                                    </style>
                                                                    <script>
                                                                        $(document).ready(function () {
                                                                            var getdate = new Date();
                                                                            var arrive = getdate;
                                                                            var depart = new Date();
                                                                            depart.setDate(getdate.getDate() + 1);
                                                                            console.log(depart);
                                                                            $("#Arrive").val($.datepicker.formatDate('mm/dd/yy', arrive));
                                                                            $("#Depart").val($.datepicker.formatDate('mm/dd/yy', depart));
                                                                            $("#Arrive").datepicker({
                                                                                minDate: arrive,
                                                                            });
                                                                            $("#Depart").datepicker({
                                                                                minDate: depart,
                                                                            });
                                                                        });
                                                                    </script>
                                                                </li>
                                                            </ul>
                                                        </div>
                                                    </aside>
                                                </section>
                                        </div>
                                        <div class="clear"></div>
                                    </div>
                                </section>
                            </section>
                        </div>
                    </div>
                </div>
            </div>
            <style>
                .z-tabs.silver>ul,
                .z-tabs.silver>ul>li>a,
                .z-tabs.silver>.z-container {
                    border: none;
                }
            </style>
            <!-- END MAIN CONTENT -->
            
